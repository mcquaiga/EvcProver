<reactiveui:ReactiveUserControl
        x:Class="Devices.Terminal.Wpf.Controls.SettingsView"
    xmlns:reactiveui="http://reactiveui.net"
    x:TypeArguments="vm:SettingsViewModel"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:materialDesign="http://materialdesigninxaml.net/winfx/xaml/themes"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:vm="clr-namespace:Devices.Terminal.Wpf.Controls"
    mc:Ignorable="d"
    d:DesignHeight="450" d:DesignWidth="800">
    <Grid>
        <materialDesign:Card Padding="32" Margin="16">
            <TextBlock Style="{DynamicResource MaterialDesignTitleTextBlock}">Settings</TextBlock>
        </materialDesign:Card>
    </Grid>
</reactiveui:ReactiveUserControl>